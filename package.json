{
  "name": "assignment-nodejs",
  "version": "1.0.0",
  "description": "Simple boilerplate for nodejs project",
  "main": "app.js",
  "scripts": {
    "start-dev": "(nodemon --inspect --ignore node_modules --exec 'npm run eslint && babel-node src/app.js --env=dev') | bunyan",
    "start-prod": "npm run build && node dist/app.js --env=prod",
    "eslint": "./node_modules/.bin/eslint .",
    "build": "babel . --ignore node_modules,test -d dist",
    "test": "jest --env=node src",
    "coverage": "jest --env=node --collectCoverageFrom=src/**.js --collectCoverageFrom=!src/app/assignment/* --collectCoverageFrom=!src/app.js --coverage src"
  },
  "author": "dat.phamtien@pycogroup.com",
  "license": "ISC",
  "dependencies": {
    "bunyan": "^1.8.12",
    "mathjs": "^5.4.0",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^5.1.0",
    "eslint-config-google": "^0.9.1",
    "jest": "^23.4.1",
    "nodemon": "^1.18.2",
    "sinon": "^6.1.4"
  }
}
